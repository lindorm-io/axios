{
  "name": "@lindorm-io/axios",
  "description": "Axios Request Handler for lindorm.io packages",
  "version": "0.2.4",
  "license": "ISC",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git@github.com:lindorm-io/axios.git"
  },
  "bugs": {
    "url": "https://github.com/lindorm-io/axios/issues"
  },
  "author": "Jonn Nilsson",
  "maintainers": [
    "jonn@jonn.nu"
  ],
  "scripts": {
    "build": "tsc",
    "build:clean": "rimraf dist",
    "check:lint": "eslint --ext .ts ./src/",
    "check:lint:fix": "eslint --ext .ts ./src/ --fix",
    "check:types": "tsc -p tsconfig.json --noEmit",
    "check:types:watch": "tsc -p tsconfig.json --noEmit --watch",
    "postpublish": "rimraf dist",
    "postversion": "git push --no-verify && git push --tags --no-verify",
    "prepublishOnly": "npm run build:clean && npm run build",
    "preversion": "npm run validate",
    "publish:major": "npm version major && npm publish",
    "publish:minor": "npm version minor && npm publish",
    "publish:patch": "npm version patch && npm publish",
    "test": "npm run test:unit",
    "test:clear": "jest --clearCache",
    "test:coverage": "rimraf coverage && jest --coverage --forceExit",
    "test:unit": "jest --config jest.config.js --",
    "test:unit:watch": "jest --config jest.config.js --watch --",
    "test:update": "jest --updateSnapshot",
    "test:watch": "npm run test:unit:watch",
    "update": "ncu -u && ncu -u",
    "validate": "npm run test:clear && npm run check:lint && npm run check:types && npm test"
  },
  "dependencies": {
    "@lindorm-io/core": "0.2.7",
    "@lindorm-io/errors": "0.1.9",
    "axios": "0.21.1",
    "lodash": "4.17.21"
  },
  "peerDependencies": {
    "@lindorm-io/winston": ">=0.1.8"
  },
  "devDependencies": {
    "@lindorm-io/winston": "0.1.8",
    "@types/jest": "27.0.0",
    "@types/lodash": "4.14.172",
    "@types/node": "16.4.13",
    "@typescript-eslint/eslint-plugin": "4.29.1",
    "@typescript-eslint/parser": "4.29.1",
    "eslint": "7.32.0",
    "husky": "7.0.1",
    "jest": "27.0.6",
    "jest-extended": "0.11.5",
    "lint-staged": "11.1.2",
    "mockdate": "3.0.5",
    "prettier": "2.3.2",
    "rimraf": "3.0.2",
    "ts-jest": "27.0.4",
    "ts-node": "10.2.0",
    "typescript": "4.3.5"
  }
}
